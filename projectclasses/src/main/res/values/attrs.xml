<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="SwitchExt">
        <attr name="hideText"         format="boolean"/>
        <attr name="fillDbColumnLabel" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="EditTextExt">
        <attr name="validate">
            <flag name="regexp"     value="0x0001" />
            <flag name="numeric"    value="0x0002" />
            <flag name="alpha"      value="0x0004" />
            <flag name="alphaNumeric" value="0x0008" />
            <flag name="email"      value="0x0010" />
            <flag name="creditCard" value="0x0020" />
            <flag name="phone"      value="0x0040" />
            <flag name="domainName" value="0x0080" />
            <flag name="ipAddress"  value="0x0100" />
            <flag name="webUrl"     value="0x0200" />
            <flag name="empty"      value="0x0400" />
            <flag name="decimal"    value="0x1000" />
            <flag name="nocheck"    value="0" />
        </attr>

        <attr name="localeTag"        format="string"/>
        <attr name="errorString"      format="string" />
        <attr name="emptyErrorString" format="string" />
        <attr name="validatorRegexp"  format="string" />
        <attr name="displayFormat"    format="string" />
        <attr name="backgroundResource" format="integer" />
        <attr name="auto"             format="boolean" />
        <attr name="drawlines"        format="boolean" />
        <attr name="editContextMenu"  format="boolean" />

        <attr name="autoScale"             format="boolean" />
        <attr name="autoLinesLimit"        format="float" />
        <attr name="autoTextScale"         format="float" />
        <attr name="autoAnimationDuration" format="integer" />

    </declare-styleable>

    <declare-styleable name="EditTextMoney">
        <attr name="maxNumberOfDecimalDigits" format="integer"/>
    </declare-styleable>

    <declare-styleable name="TextViewMoney">
        <attr name="maxNumberOfDecimalDigitsView" format="integer"/>
    </declare-styleable>

    <declare-styleable name="TextViewExt">
        <attr name="textformat">
            <flag name="regexp"     value="0x0001" />
            <flag name="dateshort"  value="0x0800" />
            <flag name="nocheck"    value="0"  />
        </attr>
        <attr name="displayFormatTv" format="string" />
        <attr name="maxNumberOfDecimalDigitsTv" format="integer"/>
        <attr name="textContextMenu" format="boolean" />
        <attr name="colorChange"     format="boolean" />
        <attr name="color"           format="color"/>
    </declare-styleable>

    <declare-styleable name="ColorPicker">
        <attr name="color_wheel_radius" format="dimension" />
        <attr name="color_wheel_thickness" format="dimension" />
        <attr name="color_center_radius" format="dimension" />
        <attr name="color_center_halo_radius" format="dimension"/>
        <attr name="color_pointer_radius" format="dimension" />
        <attr name="color_pointer_halo_radius" format="dimension" />
    </declare-styleable>
    
    <declare-styleable name="ColorBars">
        <attr name="bar_thickness" format="dimension" />
        <attr name="bar_length" format="dimension" />
        <attr name="bar_pointer_radius" format="dimension" />
        <attr name="bar_pointer_halo_radius" format="dimension" />
    </declare-styleable>
      
    <declare-styleable name="ThemeValue">
        <attr name="setTheme" format="integer" />
    </declare-styleable>

    <attr name="elements_color" format="color" />
    <attr name="ripple_color" format="color" />

    <declare-styleable name="DonutChart">
        <attr name="pieInnerCircleRatio" format="integer"/>
    </declare-styleable>

    <declare-styleable name="SemiCircle">
        <attr name="semiInnerCircleRatio" format="integer"/>
        <attr name="semi_circle" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="AreaChart">
        <attr name="show_datapoints" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="WheelPicker">
        <attr name="wheel_data" format="reference" />
        <attr name="wheel_selected_item_position" format="integer" />
        <attr name="wheel_item_text_size" format="dimension" />
        <attr name="wheel_item_text_color" format="color" />
        <attr name="wheel_selected_item_text_color" format="color" />
        <attr name="wheel_same_width" format="boolean" />
        <attr name="wheel_maximum_width_text" format="string" />
        <attr name="wheel_maximum_width_text_position" format="integer" />
        <attr name="wheel_visible_item_count" format="integer" />
        <attr name="wheel_item_space" format="dimension" />
        <attr name="wheel_cyclic" format="boolean" />
        <attr name="wheel_indicator" format="boolean" />
        <attr name="wheel_indicator_color" format="color" />
        <attr name="wheel_indicator_size" format="dimension" />
        <attr name="wheel_curtain" format="boolean" />
        <attr name="wheel_curtain_color" format="color" />
        <attr name="wheel_atmospheric" format="boolean" />
        <attr name="wheel_curved" format="boolean" />
        <attr name="wheel_item_align" format="enum">
            <enum name="center" value="0" />
            <enum name="left" value="1" />
            <enum name="right" value="2" />
        </attr>
        <attr name="wheel_font_path" format="string" />
    </declare-styleable>

    <declare-styleable name="FilePickerPreference">
        <attr name="selection_mode" format="enum">
            <enum name="single_mode" value="0"/>
            <enum name="multi_mode" value="1"/>
        </attr>
        <attr name="selection_type" format="enum">
            <enum name="file_select" value="0"/>
            <enum name="dir_select" value="1"/>
            <enum name="file_dir_select" value="2"/>
        </attr>

        <attr
            name="root_dir"
            format="string"/>

        <attr
            name="error_dir"
            format="string"/>

        <attr
            name="offset_dir"
            format="string"/>

        <attr
            name="extensions"
            format="string"/>

        <attr
            name="title_text"
            format="string"/>

    </declare-styleable>

    <declare-styleable name="TextWheelPickerView">
        <attr name="highlighting_visible" format="boolean" />
    </declare-styleable>

    <declare-styleable name="TouchImageView">
        <attr name="zoom_enabled" format="boolean" />
    </declare-styleable>

    <declare-styleable name="GalleryViewPager">
        <attr name="imageOverScrollEnabled" format="boolean" />
    </declare-styleable>

    <declare-styleable name="GestureImageView">
        <attr name="imageGesturesEnabled" format="boolean" />
        <attr name="moveUnmagnifiedImageViaSingleFingerAllowed" format="boolean" />
        <attr name="touchSensitivity" format="float" />
    </declare-styleable>

    <declare-styleable name="PdfRendererView">
        <attr name="pdfView_quality" format="enum">
            <enum name="fast" value="1" />
            <enum name="normal" value="2" />
            <enum name="enhanced" value="3" />
        </attr>
        <attr name="pdfView_engine" format="enum">
            <enum name="internal" value="100" />
            <enum name="google" value="200" />
        </attr>
        <attr name="pdfView_showDivider" format="boolean" />
        <attr name="pdfView_divider" format="reference" />

        <attr name="pdfView_enableLoadingForPages" format="reference|boolean" />

        <attr name="pdfView_page_margin" format="reference|dimension"  />
        <attr name="pdfView_page_marginLeft" format="reference|dimension"  />
        <attr name="pdfView_page_marginTop" format="reference|dimension" />
        <attr name="pdfView_page_marginRight" format="reference|dimension"  />
        <attr name="pdfView_page_marginBottom" format="reference|dimension"  />
    </declare-styleable>

    <!-- icons -->
    <attr name="pdfView_backIcon" format="reference" />
    <attr name="pdfView_downloadIcon" format="reference" />

    <!-- icon color -->
    <attr name="pdfView_downloadIconTint" format="reference|color" />
    <attr name="pdfView_actionBarTint" format="reference|color" />

    <!-- Text Appearance Customizations - Common -->
    <attr name="pdfView_titleTextStyle" format="reference" />

    <attr name="pdfView_progressBar" format="reference"/>
    <attr name="pdfView_backgroundColor" format="reference|color"/>

    <declare-styleable name="BarcodeScanning">
        <attr format="boolean" name="flashButtonVisible"/>
        <attr format="boolean" name="isAnimated"/>
    </declare-styleable>

</resources>
